{
  "name": "sovryn",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,js}\"",
    "test": "turbo run test",
    "coverage": "turbo run coverage",
    "storybook": "turbo run storybook"
  },
  "devDependencies": {
    "@sovryn/eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0"
  },
  "packageManager": "yarn@1.22.10"
}
